<div class="container padded tt">
  <h1>Create Your Alibi Offer</h1>

  <div class='col-xs-6'>
    <ul class="new-form">
      <%= simple_form_for(@alibi) do |f| %>
        <!-- [...] -->
        <%= f.input :title%>
        <%= f.input :description %>
        <%= f.input :price %>
        <%= f.input :location %>
        <%= f.input :category  %>
        <div class="btn-photo">
          <%= f.input :photo %>
          <%= f.input :photo_cache, as: :hidden %>
        </div>
        <%= f.submit %>
        <!-- [...] -->
      <% end %>
    </ul>
  </div>
    <%= link_to alibis_path do %>
     <i class="fa fa-undo backlist" aria-hidden="true">Back to list</i>
    <% end %>
</div>
