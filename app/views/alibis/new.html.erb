<div class="container padded">
  <h1>Create Your Alibi Offer</h1>
  <ul>
    <%= simple_form_for(@alibi) do |f| %>
      <!-- [...] -->
      <%= f.input :title %>
      <%= f.input :description %>
      <%= f.input :price %>
      <%= f.input :location %>
      <%= f.input :category  %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden %>
      <%= f.submit %>
      <!-- [...] -->
    <% end %>
  </ul>
    <%= link_to alibis_path do %>
     <i class="fa fa-undo" aria-hidden="true">Back to list</i>
    <% end %>
</div>
